{
    "auto-close-tag.SublimeText3Mode": true,
    "auto-close-tag.enableAutoCloseSelfClosingTag": true,
    "auto-close-tag.enableAutoCloseTag": true,
    "auto-close-tag.fullMode": false,
    "editor.accessibilitySupport": "off",
    "editor.autoIndent": false,
    "editor.cursorBlinking": "solid",
    "editor.cursorStyle": "line",
    "editor.dragAndDrop": false,
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "editor.fontSize": 15,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": false,
    "editor.lineHeight": 20,
    "editor.minimap.enabled": false,
    "editor.quickSuggestionsDelay": 100,
    "editor.renderControlCharacters": true,
    "editor.renderLineHighlight": "all",
    "editor.renderWhitespace": "boundary",
    "editor.rulers": [80,120],
    "editor.smoothScrolling": true,
    "editor.snippetSuggestions": "top",
    "editor.wordBasedSuggestions": false,
    "editor.wordWrap": "on",
    "editor.wrappingIndent": "indent",
    "emmet.preferences": {
        "css.autoInsertVendorPrefixes": false,
        "css.propertyEnd": ";",
        "css.syntaxes": [
            "css",
            "less",
            "scss"
        ],
        "sass.propertyEnd": "",
        "sass.autoInsertVendorPrefixes": false,
        "stylus.autoInsertVendorPrefixes": false,
        "stylus.propertyEnd": "",
        "stylus.valueSeparator": ": "
    },
    "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
    "emmet.triggerExpansionOnTab": true,
    "eslint.enable": false,
    "explorer.openEditors.visible": 0,
    "files.associations": {},
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true
    },
    "files.useExperimentalFileWatcher": true,
    "javascript.validate.enable": false,
    "less.scannerExclude": [
        "**/.git",
        "**/bower_components"
    ],
    "path-intellisense.extensionOnImport": false,
    "path-intellisense.mappings": {
        "~": "${workspaceRoot}/src"
    },
    "sortImports.onSave": false,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.fontFamily": "Fira Code",
    "terminal.integrated.fontLigatures": true,
    "terminal.integrated.fontSize": 16,
    "todohighlight.keywords": [
        {
            "text": "TODO:", // custom text to be highlighted
            "color": "#00aaff", // the text color, any css color identifier is valid
            "backgroundColor": "transparent" // the text background color
        },
        {
            "text": "FIXME:",
            "color": "#ec5840",
            "backgroundColor": "transparent"
        },
        {
            "text": "NOTE:",
            "color": "#5bc0de",
            "backgroundColor": "transparent"
        }
    ],
    "vsicons.presets.jsOfficial": true,
    "vsicons.presets.tsOfficial": true,
    "window.zoomLevel": 0,
    "workbench.iconTheme": "vscode-great-icons"
}